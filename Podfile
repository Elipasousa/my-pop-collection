# Uncomment this line to define a global platform for your project
platform :ios, '10.0'

use_frameworks!

def default_pods    
    pod 'Reveal-SDK', :configurations => ['Debug']
    pod 'Alamofire', '~> 4.0'
    pod 'IQKeyboardManagerSwift'
    pod 'ActionSheetPicker-3.0'
    pod 'SwiftDate'
    pod 'RealmSwift'
end

target 'MyPopCollection' do
    default_pods
end